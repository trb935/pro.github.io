<!DOCTYPE html>

<html>

<body>

<h2>Payeer</h2>

<button onclick="myFunction()">Payeer Payment address</button>

<p id="pdemo"></p>

<script>

function myFunction() {

var demo;

var city = prompt("Your payment addres:", "Mumbai");

if (city == null || city == "") {

demo = "Your transaction has been canceled.";

} else {

demo = "Payment is awaited. " + demo + "! Everyone's Dream";

}

document.getElementById("pdemo").innerHTML = demo;

}

</script>

</body>

</html>

